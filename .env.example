# Elasticsearch superuser password
# Generate a strong password (at least 16 characters)
ELASTIC_PASSWORD=changeme_generate_strong_password

# Kibana system user password (will be set automatically)
# Generate a strong password (at least 16 characters)
KIBANA_PASSWORD=changeme_generate_strong_password

# Kibana encryption key for saved objects (must be exactly 32 characters)
# Generate with: openssl rand -base64 32 | head -c 32
KIBANA_ENCRYPTION_KEY=changeme_exactly_32_characters

# Fleet enrollment token (will be generated after first setup)
FLEET_ENROLLMENT_TOKEN=

# Legacy APM secret token (still used for backward compatibility)
# Generate a strong token (at least 16 characters)
APM_SECRET_TOKEN=changeme_generate_strong_token
